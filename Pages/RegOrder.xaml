<Page x:Class="Pharm.Pages.RegOrder"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Pharm.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegOrder">

    <StackPanel x:Name="sp">
        <Label Content="Код пациента"/>
        <TextBox x:Name="tb1"/>
        <Label Content="Фамилия пациента"/>
        <TextBox x:Name="tb2"/>
        <Label Content="Имя пациента"/>
        <TextBox x:Name="tb3"/>
        <Label Content="Отчество пациента"/>
        <TextBox x:Name="tb4"/>
        <Label Content="Дата рождения пациента"/>
        <TextBox x:Name="tb5"/>
        <Label Content="Адрес пациента"/>
        <TextBox x:Name="tb6"/>
        <Button HorizontalAlignment="Center" Content="Оформить" Margin="0 10 0 0" Width="100"/>
    </StackPanel>
</Page>
